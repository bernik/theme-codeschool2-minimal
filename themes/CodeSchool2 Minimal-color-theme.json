{
    "name": "CodeSchool2 Minimal",
    "type": "dark",
    "colors": {
        "editor.background": "#182227",
        "editor.foreground": "#F0F0F0",
        "editor.lineHighlightBackground": "#FFFFFF1A",
        "editor.selectionBackground": "#DDF0FF33",

        "activityBarBadge.background": "#353a3d",

        "sideBar.background": "#182227",
        "sideBarTitle.foreground": "#F0F0F0",

        "activityBar.background": "#202429",
        "activityBar.foreground": "#F0F0F0",

        "titleBar.activeBackground": "#202429",

        "statusBar.background": "#202429",
        "statusBar.debuggingBackground": "#BFABCB",

        "editorGroupHeader.tabsBackground": "#151e22",

        "tab.inactiveBackground": "#202429",
        "tab.activeBackground": "#25353d",

        "panel.background": "#202429",

        "editor.selectionHighlightBackground": "#DDF0FF33"


    },
    "tokenColors":[
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#E5AD94F5",
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#8BB664",
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#DDF2A4",
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant",
                "constant.other"
            ],
            "settings": {
                "foreground": "#68A9EB",
            }
        },
        {
            "name": "Entities",
            "scope": [
                "entity.name",
            ],
            "settings": {
                "foreground": "#BCDBFF",
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#FD5FF1",
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
            ],
            "settings": {
                "foreground": "#b0b0b0",
            }
        },
        {
            "name": "PHP dollar sign",
            "scope": [
                "punctuation.definition.variable.php",
                "variable.other.php",
                "source.php",
                "meta.embedded.block.php",
                "text.html.php",
                "variable.other.php"
            ],
            "settings": {
                "foreground": "#F0F0F0",
            }
        },
        {
            "name": "Markup: italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup: bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: underline",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup: heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "",
            "scope": [

            ],
            "settings": {
                "foreground": "#FF0000",
            }
        },

    ]
}